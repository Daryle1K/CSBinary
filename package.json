{
  "name": "csbinary",
  "version": "2.0.5",
  "description": "A port of BinaryReader and BinaryWriter from .NET Core to NodeJS",
  "main": "dist/entry.js",
  "types": "types/entry.d.ts",
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.spec.ts",
    "test:one": "ts-mocha -p tsconfig.json test/binary-reader.spec.ts --timeout 99999999",
    "test:coverage": "nyc --reporter=html --reporter=text --reporter=text-summary npm test",
    "config": "node-gyp configure",
    "build": "node-gyp build",
    "build:debug": "node-gyp build --debug",
    "rebuild": "npm run clean && node-gyp rebuild",
    "rebuild:debug": "npm run clean && node-gyp rebuild --debug",
    "major": "npm --no-git-tag-version --no-commit-hooks version major",
    "minor": "npm --no-git-tag-version --no-commit-hooks version minor",
    "patch": "npm --no-git-tag-version --no-commit-hooks version patch",
    "prepack": "npm run clean && tsc",
    "clean": "node-gyp clean && rimraf dist types coverage test/tmp",
    "gen-doc": "typedoc"
  },
  "keywords": [
    "binaryreader",
    "binarywriter",
    "buffer",
    "read",
    "write",
    "file"
  ],
  "files": [
    "src",
    "dist",
    "types",
    "binding.gyp",
    "entry.ts",
    "README.md",
    "tsconfig.json",
    "LICENSE"
  ],
  "author": "Meigyoku Thmn",
  "homepage": "https://meigyoku-thmn.github.io/CSBinary",
  "bugs": "https://github.com/Meigyoku-Thmn/CSBinary/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/Meigyoku-Thmn/CSBinary.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 10.0.0"
  },
  "devDependencies": {
    "@types/bindings": "^1.3.0",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.6.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.19.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "iconv-lite": "npm:@meigyokuthmn/iconv-lite@^0.7.0-pre",
    "node-addon-api": "^3.0.2"
  },
  "gypfile": true
}
