{
  "name": "csbinary",
  "version": "1.0.0",
  "description": "A port of BinaryReader and BinaryWriter from .NET Core to NodeJS",
  "main": "dist/entry.js",
  "types": "types/entry.d.ts",
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.spec.ts",
    "test:one": "ts-mocha -p tsconfig.json test/binary-reader.spec.ts --timeout 99999999",
    "test:coverage": "nyc --reporter=html --reporter=text --reporter=text-summary npm test",
    "config": "node-gyp configure",
    "build": "node-gyp build && tsc",
    "rebuild": "node-gyp rebuild && tsc -b --clean && rimraf types && tsc",
    "clean": "node-gyp clean && rimraf dist && rimraf types && rimraf coverage",
    "postinstall": "tsc"
  },
  "files": [
    "src",
    "binding.gyp",
    "entry.ts",
    "README.md",
    "tsconfig.json",
    "LICENSE"
  ],
  "author": "Meigyoku Thmn",
  "license": "MIT",
  "devDependencies": {
    "@types/bindings": "^1.3.0",
    "@types/fs-ext": "^2.0.0",
    "@types/lodash": "^4.14.157",
    "@types/mocha": "^7.0.2",
    "@types/mock-require": "^2.0.0",
    "lodash": "^4.17.19",
    "memfs": "github:Meigyoku-Thmn/memfs#dev",
    "mocha": "^8.1.1",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@types/node": "^14.0.14",
    "bindings": "^1.5.0",
    "fs-ext": "github:Meigyoku-Thmn/node-fs-ext#master",
    "iconv-lite": "github:Meigyoku-Thmn/iconv-lite#master",
    "nan": "^2.14.1"
  },
  "gypfile": true
}
