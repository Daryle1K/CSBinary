{
  "name": "csbinary",
  "version": "2.0.1",
  "description": "A port of BinaryReader and BinaryWriter from .NET Core to NodeJS",
  "main": "dist/entry.js",
  "types": "types/entry.d.ts",
  "scripts": {
    "test": "ts-mocha -p tsconfig.json test/**/*.spec.ts",
    "test:one": "ts-mocha -p tsconfig.json test/binary-reader.spec.ts --timeout 99999999",
    "test:coverage": "nyc --reporter=html --reporter=text --reporter=text-summary npm test",
    "config": "node-gyp configure",
    "build": "node-gyp build",
    "rebuild": "node-gyp rebuild && rimraf types",
    "clean": "node-gyp clean && rimraf dist types coverage test/tmp",
    "postinstall": "tsc",
    "gen-doc": "typedoc"
  },
  "keywords": [
    "binaryreader",
    "binarywriter",
    "buffer",
    "read",
    "write",
    "file"
  ],
  "files": [
    "src",
    "binding.gyp",
    "entry.ts",
    "README.md",
    "tsconfig.json",
    "LICENSE"
  ],
  "author": "Meigyoku Thmn",
  "homepage": "https://meigyoku-thmn.github.io/CSBinary",
  "bugs": "https://github.com/Meigyoku-Thmn/CSBinary/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/Meigyoku-Thmn/CSBinary.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 12.0.0"
  },
  "devDependencies": {
    "@types/bindings": "^1.3.0",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^7.0.2",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.19.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/node": "^14.6.2",
    "bindings": "^1.5.0",
    "iconv-lite": "npm:@meigyokuthmn/iconv-lite@^0.7.0-pre",
    "nan": "^2.14.1"
  },
  "gypfile": true
}
